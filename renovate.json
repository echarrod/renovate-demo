{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "constraints": {
    "go": "1.21"
  },
  "ignoreUnstable": true,
  "addLabels": [
    "Bot::Renovate"
  ],
  "semanticCommits": "disabled",
  "commitMessagePrefix": "renovate:",
  "postUpdateOptions": [
    "gomodTidy",
    "gomodUpdateImportPaths"
  ]
}
